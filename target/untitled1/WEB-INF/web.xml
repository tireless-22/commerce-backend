<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>



  <filter>
    <filter-name>CORS</filter-name>
    <filter-class>com.thetransactioncompany.cors.CORSFilter</filter-class>

  </filter>
  <filter-mapping>
    <filter-name>CORS</filter-name>
    <url-pattern>/*</url-pattern>

  </filter-mapping>

  <servlet>
    <servlet-name>test</servlet-name>
    <servlet-class>com.example.test</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>




  <servlet>
    <servlet-name>userlogin</servlet-name>
    <servlet-class>com.example.userlogin</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>userlogin</servlet-name>
    <url-pattern>/userlogin</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>listItems</servlet-name>
    <servlet-class>com.example.listItems</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>listItems</servlet-name>
    <url-pattern>/listItems</url-pattern>
  </servlet-mapping>



  <servlet>
    <servlet-name>addItemsToCart</servlet-name>
    <servlet-class>com.example.addItemsToCart</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>addItemsToCart</servlet-name>
    <url-pattern>/addItemsToCart</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>modifyCartItem</servlet-name>
    <servlet-class>com.example.modifyCartItem</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>modifyCartItem</servlet-name>
    <url-pattern>/modifyCartItem</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>cartRemoveItem</servlet-name>
    <servlet-class>com.example.cartRemoveItem</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>cartRemoveItem</servlet-name>
    <url-pattern>/cartRemoveItem</url-pattern>
  </servlet-mapping>


</web-app>
